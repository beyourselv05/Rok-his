#include <stdio.h>
int main() {
    printf("\n        <곱셈표>        \n\n\n");
    printf(" |1  2  3  4  5  6  7  8  9\n");
    printf("-+--------------------------\n");
    printf("1");
    for(int i = 1; i <= 9; i++) {
        for(int j = 1; j <= 9; j++)
            printf("% 3d", i * j);
        puts("\n");
        if (i == 9) break;
        printf("%d", i+1);
    }
}
